{
  "private": true,
  "workspaces": [
    "examples/*",
    "stackpress"
  ],
  "scripts": {
    "build": "yarn --cwd stackpress build",
    "fs:build": "yarn --cwd examples/with-fieldsets build",
    "fs:dev": "yarn --cwd examples/with-fieldsets develop",
    "fs:emit": "yarn --cwd examples/with-fieldsets emit",
    "fs:generate": "yarn --cwd examples/with-fieldsets generate",
    "fs:migrate": "yarn --cwd examples/with-fieldsets migrate",
    "fs:populate": "yarn --cwd examples/with-fieldsets populate",
    "fs:purge": "yarn --cwd examples/with-fieldsets purge",
    "fs:push": "yarn --cwd examples/with-fieldsets push",
    "fs:query": "yarn --cwd examples/with-fieldsets query",
    "fs:start": "yarn --cwd examples/with-fieldsets start",
    "fs:test": "yarn --cwd examples/with-fieldsets test",
    "uno:build": "yarn --cwd examples/with-unocss build",
    "uno:dev": "yarn --cwd examples/with-unocss develop",
    "uno:emit": "yarn --cwd examples/with-unocss emit",
    "uno:generate": "yarn --cwd examples/with-unocss generate",
    "uno:migrate": "yarn --cwd examples/with-unocss migrate",
    "uno:populate": "yarn --cwd examples/with-unocss populate",
    "uno:purge": "yarn --cwd examples/with-unocss purge",
    "uno:push": "yarn --cwd examples/with-unocss push",
    "uno:query": "yarn --cwd examples/with-unocss query",
    "uno:start": "yarn --cwd examples/with-unocss start",
    "uno:test": "yarn --cwd examples/with-unocss test"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5"
  }
}